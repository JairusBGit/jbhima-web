{
  "hosting": {
   "site": "himawari-jb",
    "public": "public",
    "cleanUrls": true,
    "trailingSlash": true,

    "redirects": [ 
      {
        "source": "konclient.html",
        "destination": "/konclient",
        "type": 301
      }, 
  
      {
        "source": "404.html",
        "destination": "/404",
        "type": 301
      },
  
      {
        "source": "stuff/aboutme.html",
        "destination": "/aboutme",
        "type": 301
      }, 
      
      {
        "source": "stuff/buttons.html",
        "destination": "/buttons",
        "type": 301
      },
      
      {
        "source": "stuff/stuffer/dni.html",
        "destination": "/dontinteract",
        "type": 301
      },

      {
        "source": "stuff/stuffer/likes.html",
        "destination": "/likes",
        "type": 301
      },

      {
        "source": "stuff/stuffer/projects.html",
        "destination": "/projects",
        "type": 301
      }
      ],

    "ignore": [
      "firebase.json",
      "**/.*"]
  }
}
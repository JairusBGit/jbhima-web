{
  "hosting": {
   "site": "himawari-jb",
    "public": "public",
    "cleanUrls": true
    "trailingSlash": true
    "ignore": [
      "firebase.json",
      "**/.*"]
    "redirects": [ {
      "source": "konclient.html",
      "destination": "/konclient",
      "type": 301
    }, {
      "source": "404.html",
      "destination": "/404",
      "type": 301
    }, {
      "source": "stuff/aboutme.html",
      "destination": "/aboutme",
      "type": 301
    }, {
      "source": "stuff/buttons.html",
      "destination": "/buttons",
      "type": 301
    } ]
  }
}
